		Thread.sleep(200);
		wm.Listandenter("//i[@class='fa fa-plus']", "3");
		Thread.sleep(2500);
		
//Enter the miscdetails
		
wm.sendtextbyxvalue("//input[@aria-owns='MiscServiceType_listbox']", Servicetype);
Thread.sleep(200);
wm.Listandenter("//*[@id='MiscServiceType_listbox']/li",Commonindex);
